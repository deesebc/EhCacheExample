<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="true"
	monitoring="autodetect" dynamicConfig="true">
	<defaultCache eternal="false" timeToIdleSeconds="120"
		timeToLiveSeconds="120" diskExpiryThreadIntervalSeconds="120" maxEntriesLocalHeap="5000"
		memoryStoreEvictionPolicy="LRU" />
	<cache name="es.home.ehcache.example.domain.Libro"
		maxElementsInMemory="2000" eternal="false" timeToIdleSeconds="1800"
		timeToLiveSeconds="3600" overflowToDisk="false" />
</ehcache>